{
  "name": "powrstack-folio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:cloudflare": "next build && pnpm exec opennextjs-cloudflare build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "validate": "pnpm lint && pnpm test && pnpm build",
    "preview:local": "pnpm build:cloudflare && wrangler dev .open-next/worker.js --port 8787",
    "deploy": "npm run build:cloudflare && wrangler pages deploy .open-next/assets --project-name=powrstack-folio",
    "deploy:worker": "pnpm build:cloudflare && wrangler deploy .open-next/worker.js",
    "deploy:staging": "pnpm build:cloudflare && wrangler deploy .open-next/worker.js --env staging",
    "deploy:production": "pnpm validate && pnpm build:cloudflare && wrangler deploy .open-next/worker.js --env production",
    "preview": "wrangler pages dev .open-next/assets --compatibility-date=2025-03-25",
    "preview:worker": "npm run build:cloudflare && wrangler dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.6",
    "framer-motion": "^12.23.26",
    "next": "16.0.8",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "16.0.8",
    "@opennextjs/cloudflare": "^1.14.4",
    "@tailwindcss/postcss": "^4.1.17",
    "daisyui": "^5.5.11",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.8",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4.1.17",
    "vitest": "^3.2.4",
    "wrangler": "^4.53.0"
  }
}
